{
  "extends": "airbnb-base",
  "rules": {
    "func-names": ["error", "never"],
    "indent": ["error", 2],
    "no-console": 0,
    "import/no-extraneous-dependencies": 0
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["."]
      }
    },
  },
  "env": {
    "jest": true
  }
}
